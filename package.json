{
  "name": "dinner-party-gantt",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch dist -e js,js.map ./dist/main.js",
    "build:frontend": "webpack-dev-server --port 3000",
    "build:backend": "tsc --watch --skipLibCheck",
    "eslint": "eslint . --format visualstudio --report-unused-disable-directives",
    "eslint:fix": "npm --silent run eslint -- --fix",
    "eslint:subset": "eslint --format visualstudio --report-unused-disable-directives",
    "eslint:staged": "lint-staged",
    "prettier": "prettier --check \"{src}/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"{src}/**/*.{js,jsx,ts,tsx}\"",
    "prettier:staged": "pretty-quick --staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunningman/dinner-party-gantt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunningman/dinner-party-gantt/issues"
  },
  "homepage": "https://github.com/brunningman/dinner-party-gantt#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:staged && npm run eslint:staged",
      "pre-push": "bash ./scripts/confirm-push-to-master.sh"
    }
  },
  "dependencies": {
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gantt": "^2.1.8",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/express": "^4.17.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "source-map-loader": "^1.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
